# Neo4j Configuration
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=password

# MCP Server Configuration  
MCP_SERVER_NAME=my-wiki-mcp
MCP_SERVER_VERSION=1.0.0

# HTTP Transport Configuration (for development and MCP Inspector)
MCP_HTTP_PORT=3000
MCP_HTTP_PATH=/mcp

# Development Mode
# Set to 'http' for streamable HTTP transport with MCP Inspector
# Set to 'stdio' for standard input/output transport
MCP_TRANSPORT=http